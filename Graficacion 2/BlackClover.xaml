<Window x:Class="Graficacion_2.BlackClover"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Graficacion_2"
        mc:Ignorable="d"
        Title="BlackClover" Height="450" Width="800">
    <Canvas>
        <Canvas.Background>
            <VisualBrush Viewport="0,0 0.5 0.5" TileMode="FlipXY" Stretch="Uniform">
                <VisualBrush.Visual>
                    <Path Stroke="Black" StrokeThickness="4" Canvas.Left="200" Canvas.Top="150" Data="M00,100 50,100 40,60 A30,30 0 1 0 40,10
                      A30,30 0 1 0 0,15 A 30,30 0 1 0 0,55 L-10,100z" Fill="Black" ></Path>
                </VisualBrush.Visual>
            </VisualBrush>
        </Canvas.Background>
       
    </Canvas>
</Window>
