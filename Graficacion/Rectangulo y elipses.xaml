<Window x:Class="Graficacion.Rectangulo_y_elipses"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Graficacion"
        mc:Ignorable="d"
        Title="Rectangulo_y_elipses" Height="450" Width="800">
    <Canvas>
        <Rectangle Canvas.Left="40" Canvas.Top="20"  Stroke="YellowGreen" Width="100"
                  Height="100" Fill="Yellow"></Rectangle>
        <Rectangle Canvas.Left="200" Canvas.Top="20"  Stroke="YellowGreen" Width="50"
                  Height="100" Fill="Yellow" RadiusX="5" RadiusY="5"></Rectangle>
        <Ellipse Canvas.Left="300" Canvas.Top="20" Stroke="YellowGreen"
                 Width="50" Height="100" Fill="Yellow"></Ellipse>

    </Canvas>
</Window>
